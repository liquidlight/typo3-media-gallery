{
    "name": "liquidlight/typo3-media-gallery",
    "description": "Advanced multi-engine front-end gallery component",
    "type": "typo3-cms-extension",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Liquid Light",
            "email": "info@liquidlight.co.uk",
            "homepage": "https://www.liquidlight.co.uk/"
        },
        {
            "name": "Jo√£o Augusto",
            "email": "joao@liquidlight.co.uk"
        },
        {
            "name": "Mike Street",
            "email": "mike@liquidlight.co.uk"
        }
    ],
    "require": {
        "typo3/cms-core": "^10.4 || ^11.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "LiquidLight\\MediaGallery\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "media_gallery"
        }
    },
    "replace": {
        "liquidlight/media-gallery": "self.version"
    }
}
